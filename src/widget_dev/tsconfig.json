{
    "compilerOptions": {
        "esModuleInterop": true,
        "module": "amd",
        "noImplicitAny": true,
        "sourceMap": true,
        "jsx": "react",
        "jsxFactory": "tsx",
        "target": "es5",
        "experimentalDecorators": true,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "include": ["./*"]
    // "exclude": ["node_modules"]
}
